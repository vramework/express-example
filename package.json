  "description": "An example to a minimal express deployment with vramework",
  "main": "bin/main.ts",
  "dependencies": {
    "@vramework/core": "^0.1.16",
    "@vramework/deploy-express": "^0.1.16",
    "@vramework/services-local": "^0.1.16"
  },
  "devDependencies": {
    "@vramework/cli": "^0.1.16",
