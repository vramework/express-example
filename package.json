{
  "name": "@vramework/express-starter",
  "version": "0.0.0",
  "description": "An example to a minimal express deployment with vramework",
  "main": "bin/main.ts",
  "type": "module",
  "dependencies": {
    "@vramework/cli": "^0.6.2",
    "@vramework/core": "^0.6.3",
    "@vramework/express-middleware": "^0.6.2",
    "@vramework/fetch": "^0.6.0",
    "@vramework/jose": "^0.6.0",
    "@vramework/schedule": "^0.6.0",
    "cookie-parser": "^1.4.7",
    "express": "^5"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.7",
    "@types/supertest": "^6.0.2",
    "@vramework/cli": "^0.6.2",
    "oxlint": "^0.9.5",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.6"
  },
  "scripts": {
    "prebuild": "npx --yes @vramework/cli",
    "tsc": "tsc",
    "test": "bash run-tests.sh",
    "start": "tsx bin/main.ts",
    "dev": "tsx --watch bin/main.ts",
    "lint": "npx oxlint@latest",
    "prettier": "npx prettier . --write",
    "ncu:vramework": "ncu -f '/@vramework/.*/' -u && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vramework/express-example.git"
  },
  "keywords": [
    "server",
    "nodejs",
    "http"
  ],
  "author": "Yasser Fadl",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vramework/express-example/issues"
  },
  "homepage": "https://github.com/vramework/express-example#readme",
  "packageManager": "^npm@10.8.2",
  "engines": {
    "node": ">=18"
  }
}
